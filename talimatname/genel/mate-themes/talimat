# Description: MATE temalarÄ±
# URL: http://matsusoft.com.ar/projects/mate
# Packager: milisarge yasarciv
# Depends on: gtk2


name=mate-themes
version=3.20.7
release=1
source=(http://pub.mate-desktop.org/releases/themes/${version%.*}/${name}-${version}.tar.xz)

build() {
	cd ${name}-${version}
    ./configure \
        --prefix=/usr 
make
make DESTDIR=$PKG install
}
