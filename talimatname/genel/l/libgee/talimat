# Grup: kütüphane

isim=libgee
surum=0.20.0
devir=1

kaynak=(https://git.gnome.org/browse/libgee/snapshot/libgee-0.20.0.tar.xz)

derle() {
cd $isim-$surum
  NOCONFIGURE=1 ./autogen.sh
./configure --prefix=/usr
make 
make DESTDIR=$PKG install
}
