# Tanım: LXDE için freedesktop.org menü spesifik uyumlu masaüstü menüleri oluşturmak için gerekli dosyalar.
# URL: http://lxde.org/
# Paketçi: milisarge
# Gerekler: intltool

isim=lxde-menu-data
surum=0.1.5
devir=2
_isim=lxmenu-data

kaynak=(http://downloads.sourceforge.net/lxde/${_isim}-$surum.tar.xz)
derle() {
cd $_isim-$surum
./configure --prefix=/usr --sysconfdir=/etc
make
make DESTDIR=$PKG install
}

