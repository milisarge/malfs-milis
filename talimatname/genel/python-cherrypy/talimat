# Depends on: python 
name=python-cherrypy
version=3.6.0
release=1

source=(
https://pypi.python.org/packages/source/C/CherryPy/CherryPy-$version.tar.gz)

build() {
    cd CherryPy-$version
    python2 setup.py install --prefix=/usr --root=$PKG
}



