name=shared-color-targets
version=0.1.7
release=1
source=(http://people.freedesktop.org/~hughsient/releases/$name-$version.tar.xz)

build(){
	cd $name-$version
./configure --prefix=/usr       
  make
  make DESTDIR=$PKG install
}
