# Tanım: TNEF verilerini işleme API'si
# URL: https://projects.kde.org/projects/kde/pim/
# Paketçi: alihan-ozturk28@hotmail.com
# Gerekler: kf5-extra-cmake-modules python kf5-kdoctools kcalutils kcontacts
# Grup: kde

isim=ktnef
surum=17.12.0
devir=1

kaynak=( http://download.kde.org/stable/applications/$surum/src/$isim-$surum.tar.xz)

derle() {
  cd $isim-$surum
  cmake -DCMAKE_BUILD_TYPE=Release \
        -DCMAKE_INSTALL_PREFIX=/usr \
        -DCMAKE_INSTALL_LIBDIR=lib \
        -DBUILD_TESTING=OFF
        
  make
  make DESTDIR=$PKG install
}
